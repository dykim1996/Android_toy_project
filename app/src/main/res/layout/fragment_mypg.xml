<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".ResultActivity" android:background="#FFFFFF">

	<ImageView
			android:background="@drawable/img"
			android:layout_width="150dp"
			android:layout_height="150dp"
			tools:src="@drawable/img"
			android:id="@+id/imageView2"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginTop="40dp"
			app:layout_constraintHorizontal_bias="0.18"/>
	<Button
			android:text="로그아웃"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/btnLogout"
			android:backgroundTint="#306839"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="56dp" app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintVertical_bias="0.306"/>
	<TextView
			android:text="currentUser"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/currentUser"
			app:layout_constraintTop_toTopOf="parent"
			android:layout_marginTop="72dp"
			app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toEndOf="@+id/imageView2"
			app:layout_constraintHorizontal_bias="0.475"/>
	<TextView
			android:text="TextView"
			android:layout_width="64dp"
			android:layout_height="19dp" android:id="@+id/textView3"
			app:layout_constraintStart_toEndOf="@+id/textView4"
			app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.51"
			app:layout_constraintTop_toBottomOf="@+id/currentUser" android:layout_marginTop="28dp"/>
	<TextView
			android:text="가입일"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/textView4"
			app:layout_constraintStart_toEndOf="@+id/imageView2"
			app:layout_constraintTop_toBottomOf="@+id/currentUser" android:layout_marginTop="28dp"
			android:layout_marginStart="40dp"/>
	<Button
			android:text="내 사진등록"
			android:layout_width="114dp"
			android:id="@+id/Gallery_button"
			android:backgroundTint="#306839"
			android:layout_height="42dp"
			app:layout_constraintTop_toBottomOf="@+id/imageView2" app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toStartOf="@+id/btnLogout" android:layout_marginTop="4dp"
			app:layout_constraintHorizontal_bias="0.445"/>


<!--첨부파일 테스트용	-->


</androidx.constraintlayout.widget.ConstraintLayout>